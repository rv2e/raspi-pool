{
  "name": "raspi-pool",
  "version": "0.0.1",
  "description": "Raspi pool backend",
  "author": "Herve Eerebout",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:docker": "yarn db:setup && yarn start:dev",
    "lint": "eslint . --ext ts,js --ignore-path .gitignore",
    "test": "NODE_ENV=test yarn jest",
    "test:watch": "yarn test --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "yarn test --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/database/config.ts",
    "db:migration:generate": "yarn typeorm migration:generate",
    "db:migration:run": "yarn typeorm migration:run",
    "db:create": "ts-node -r tsconfig-paths/register ./src/database/scripts/createDatabase",
    "db:seed": "ts-node -r tsconfig-paths/register ./src/database/scripts/seedDatabase",
    "db:setup": "yarn db:create && yarn db:migration:run"
  },
  "dependencies": {
    "@nestjs/common": "7.0.5",
    "@nestjs/core": "7.0.5",
    "@nestjs/platform-express": "7.0.5",
    "@nestjs/platform-socket.io": "7.0.5",
    "@nestjs/schedule": "^0.3.0",
    "@nestjs/typeorm": "7.0.0",
    "@nestjs/websockets": "7.0.5",
    "@sentry/node": "5.15.0",
    "@types/superagent": "4.1.7",
    "class-transformer": "0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "8.2.0",
    "ds18b20-raspi": "^0.3.4",
    "ejs": "3.0.1",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "node-dht-sensor": "^0.4.1",
    "onoff": "^6.0.0",
    "pg": "7.18.2",
    "reflect-metadata": "0.1.13",
    "rpio": "^2.1.1",
    "rxjs": "6.5.4",
    "socket.io": "2.3.0",
    "socket.io-client": "2.3.0",
    "source-map-support": "0.5.16",
    "superagent": "5.2.2",
    "typeorm": "0.2.24",
    "uuid": "7.0.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "7.0.2",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.5",
    "@sentry/core": "5.15.0",
    "@types/cookie": "0.3.3",
    "@types/cron": "^1.7.2",
    "@types/express": "4.17.3",
    "@types/jest": "24.9.0",
    "@types/lodash": "4.14.149",
    "@types/node": "13.9.3",
    "@types/rpio": "^0.0.3",
    "@types/socket.io": "2.1.4",
    "@types/socket.io-client": "1.4.32",
    "@types/supertest": "2.0.8",
    "@types/uuid": "7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "codecov": "3.7.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "24.9.0",
    "nock": "12.0.3",
    "prettier": "2.0.1",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "24.3.0",
    "ts-loader": "6.2.2",
    "ts-node": "8.8.1",
    "typescript": "3.7.5"
  }
}
